	document.getElementById("progress3").style.width = 20;
	document.getElementById("progress4").style.width = 20;
	}
	function Odkaz()
	{
	window.open( "http://www.Twitter.com/" );
	}
	function ChangeFB()
	{
	if(FB)
	{
		document.getElementById("FB").src="FBlike2.png";
		FB=false;
	}
	else
	{
	 document.getElementById("FB").src="FBlike.png";
	 FB=true;
	}
	}
	function ChangeImg1(a)
	{
	if( a==0)
	{
		document.getElementById("img1").src="Mustache1N.png";
	}
	else
	{
	 document.getElementById("img1").src="Mustache1.png";
	}
	}
	function ChangeImg2(a)
	{
	if( a==0)
	{
	 document.getElementById("img2").src="Mustache2N.png";
	}
	else
	{
	 document.getElementById("img2").src="Mustache2.png";
	}
	}
	function ChangeImg3(a)
	{
	if( a==0)
	{
	 document.getElementById("img3").src="Mustache3N.png";
	}
	else
	{
	 document.getElementById("img3").src="Mustache3.png";
	}
}
function Zmizet(Objekt) {
  Objekt.style.opacity = Math.round((Objekt.style.opacity-0.1) * 100) / 100;
  if (Objekt.style.opacity) 
  {
    setTimeout(function() { Zmizet(Objekt) }, 30);
  }
}
