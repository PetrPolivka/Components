	var FB=true;	
	function Week1()
	{
	document.getElementById("progress1").style.width = 124;
	document.getElementById("progress2").style.width = 156;
	document.getElementById("progress3").style.width = 200;
	document.getElementById("progress4").style.width = 56;
	document.getElementById("Circle1").percent = 20;
	}
	function Week2()
	{
	document.getElementById("progress1").style.width = 230;
	document.getElementById("progress2").style.width = 59;
	document.getElementById("progress3").style.width = 146;
	document.getElementById("progress4").style.width = 12;
	}
	function Week3()
	{
	document.getElementById("progress1").style.width = 124;
	document.getElementById("progress2").style.width = 156;
	document.getElementById("progress3").style.width = 165;
	document.getElementById("progress4").style.width = 204;
	}
